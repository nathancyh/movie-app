            </li>
          </div>
          <div class="rightmargindiv">
            <li class="nav-item">
              {{#if user}}
              <a class="nav-link" href="/profile/{{user.id}}">Hello {{user.name}}</a>
              {{else}}
              <a class="nav-link" href="/login">Login <i class="fas fa-user-circle"></i></a>
              {{/if}}
            </li>
          </div>
        </ul>
      </div>
    </div>
  </nav>
  <main class="main-content">
    <div class="container">
      <div class="page">
        {{#if user}}
        <a href='/logout'><button type="button" class="btn btn-danger logoutbtn mb-3">Logout</button></a>
        {{/if}}
        <div class="breadcrumbs">
          <a href="/">Home</a>
          <i class="fas fa-angle-double-right"></i> <span>Breadcrumbs Here</span>
        </div>
        {{{body}}}
      </div>
    </div>
  </main>



