<body>

  {{!-- NAV --}}
  <nav class="navbar navbar-expand-md navbar-dark bg-dark">
    <button class="navbar-toggler order-0" type="button" data-toggle="collapse" data-target="#navbar7">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="d-flex flex-grow-1 w-75 p-2">
      <a href="/" class="navbar-brand">Movie App</a>
    </div>
    <div class="search">
      <form class="d-flex">
        <input class="typeahead form-control me-2" type="search" placeholder="Search" aria-label="Search">
      </form>
    </div>
    </div>
    <div class="navbar-collapse collapse flex-shrink-1 flex-grow-0 order-last" id="navbar7">
      <div class="rightnav">
        <ul class="navbar-nav ml-auto navbar-right">
          <div class="rightmargindiv">
